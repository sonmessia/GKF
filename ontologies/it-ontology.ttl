@prefix : <http://gkf.org/ontology/it#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix schema: <http://schema.org/> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://gkf.org/ontology/it> a owl:Ontology ;
    dc:title "IT Education Knowledge Graph Ontology" ;
    dc:description "Core ontology for the Genesis Knowledge Framework - IT Domain" ;
    dc:creator "GKF Project" ;
    dc:created "2025-10-13"^^xsd:date ;
    owl:versionInfo "1.0.0" .

#################################################################
#    Classes - Knowledge Domain Hierarchy
#################################################################

:Knowledge a owl:Class ;
    rdfs:label "Knowledge"@en ;
    rdfs:comment "Abstract representation of knowledge" .

:FoundationalKnowledge a owl:Class ;
    rdfs:subClassOf :Knowledge ;
    rdfs:label "Foundational Knowledge"@en ;
    rdfs:comment "Objective, globally verified knowledge from authoritative sources" .

:ExperientialKnowledge a owl:Class ;
    rdfs:subClassOf :Knowledge ;
    rdfs:label "Experiential Knowledge"@en ;
    rdfs:comment "Community-driven, user-generated knowledge based on interactions" .

#################################################################
#    Classes - IT Domain Core Entities
#################################################################

:Skill a owl:Class ;
    rdfs:subClassOf :Knowledge ;
    rdfs:label "Skill"@en ;
    rdfs:comment "A technical or soft skill in the IT domain" ;
    skos:example "Python Programming, Machine Learning, Leadership" .

:TechnicalSkill a owl:Class ;
    rdfs:subClassOf :Skill ;
    rdfs:label "Technical Skill"@en ;
    rdfs:comment "Technical competency required in IT" .

:SoftSkill a owl:Class ;
    rdfs:subClassOf :Skill ;
    rdfs:label "Soft Skill"@en ;
    rdfs:comment "Non-technical interpersonal skills" .

:Job a owl:Class ;
    rdfs:label "Job"@en ;
    rdfs:comment "A professional role or position in IT" ;
    skos:example "Software Engineer, Data Scientist, DevOps Engineer" .

:Course a owl:Class ;
    rdfs:label "Course"@en ;
    rdfs:comment "An educational resource for learning" ;
    skos:example "CS50, Full Stack Web Development Bootcamp" .

:LearningPath a owl:Class ;
    rdfs:label "Learning Path"@en ;
    rdfs:comment "A structured sequence of learning resources" .

:Technology a owl:Class ;
    rdfs:label "Technology"@en ;
    rdfs:comment "A technology, tool, framework, or platform" ;
    skos:example "React, Docker, PostgreSQL" .

:Certification a owl:Class ;
    rdfs:label "Certification"@en ;
    rdfs:comment "Professional certification in IT" ;
    skos:example "AWS Certified Solutions Architect, Google Cloud Professional" .

:Company a owl:Class ;
    rdfs:label "Company"@en ;
    rdfs:comment "Organization offering IT jobs" .

:UserProfile a owl:Class ;
    rdfs:label "User Profile"@en ;
    rdfs:comment "Represents a learner's profile and progress" .

:Interaction a owl:Class ;
    rdfs:subClassOf :ExperientialKnowledge ;
    rdfs:label "Interaction"@en ;
    rdfs:comment "User interaction captured as experiential knowledge" .

#################################################################
#    Object Properties
#################################################################

:requires a owl:ObjectProperty ;
    rdfs:domain :Job ;
    rdfs:range :Skill ;
    rdfs:label "requires"@en ;
    rdfs:comment "Job requires a specific skill" .

:teaches a owl:ObjectProperty ;
    rdfs:domain :Course ;
    rdfs:range :Skill ;
    rdfs:label "teaches"@en ;
    rdfs:comment "Course teaches a particular skill" .

:prerequisite a owl:ObjectProperty, owl:TransitiveProperty ;
    rdfs:domain :Skill ;
    rdfs:range :Skill ;
    rdfs:label "prerequisite"@en ;
    rdfs:comment "Skill A is prerequisite to Skill B" .

:relatedTo a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:domain :Skill ;
    rdfs:range :Skill ;
    rdfs:label "related to"@en ;
    rdfs:comment "Skill is related to another skill" .

:partOf a owl:ObjectProperty, owl:TransitiveProperty ;
    rdfs:domain :Course ;
    rdfs:range :LearningPath ;
    rdfs:label "part of"@en ;
    rdfs:comment "Course is part of a learning path" .

:usesTechnology a owl:ObjectProperty ;
    rdfs:domain :Job ;
    rdfs:range :Technology ;
    rdfs:label "uses technology"@en ;
    rdfs:comment "Job uses specific technology" .

:recommendedFor a owl:ObjectProperty ;
    rdfs:domain :Course ;
    rdfs:range :Job ;
    rdfs:label "recommended for"@en ;
    rdfs:comment "Course is recommended for a specific job role" .

:offeredBy a owl:ObjectProperty ;
    rdfs:domain :Job ;
    rdfs:range :Company ;
    rdfs:label "offered by"@en ;
    rdfs:comment "Job is offered by a company" .

:hasSkill a owl:ObjectProperty ;
    rdfs:domain :UserProfile ;
    rdfs:range :Skill ;
    rdfs:label "has skill"@en ;
    rdfs:comment "User has acquired a skill" .

:completedCourse a owl:ObjectProperty ;
    rdfs:domain :UserProfile ;
    rdfs:range :Course ;
    rdfs:label "completed course"@en ;
    rdfs:comment "User has completed a course" .

:interestedIn a owl:ObjectProperty ;
    rdfs:domain :UserProfile ;
    rdfs:range :Job ;
    rdfs:label "interested in"@en ;
    rdfs:comment "User is interested in a job role" .

:linkedTo a owl:ObjectProperty ;
    rdfs:label "linked to"@en ;
    rdfs:comment "Links local entity to global URI (LOD)" .

#################################################################
#    Data Properties
#################################################################

:skillName a owl:DatatypeProperty ;
    rdfs:domain :Skill ;
    rdfs:range xsd:string ;
    rdfs:label "skill name"@en .

:skillLevel a owl:DatatypeProperty ;
    rdfs:domain :Skill ;
    rdfs:range xsd:string ;
    rdfs:label "skill level"@en ;
    rdfs:comment "Beginner, Intermediate, Advanced, Expert" .

:jobTitle a owl:DatatypeProperty ;
    rdfs:domain :Job ;
    rdfs:range xsd:string ;
    rdfs:label "job title"@en .

:salary a owl:DatatypeProperty ;
    rdfs:domain :Job ;
    rdfs:range xsd:decimal ;
    rdfs:label "salary"@en .

:courseName a owl:DatatypeProperty ;
    rdfs:domain :Course ;
    rdfs:range xsd:string ;
    rdfs:label "course name"@en .

:courseURL a owl:DatatypeProperty ;
    rdfs:domain :Course ;
    rdfs:range xsd:anyURI ;
    rdfs:label "course URL"@en .

:duration a owl:DatatypeProperty ;
    rdfs:domain :Course ;
    rdfs:range xsd:integer ;
    rdfs:label "duration"@en ;
    rdfs:comment "Duration in hours" .

:difficulty a owl:DatatypeProperty ;
    rdfs:domain :Course ;
    rdfs:range xsd:string ;
    rdfs:label "difficulty"@en .

:rating a owl:DatatypeProperty ;
    rdfs:range xsd:decimal ;
    rdfs:label "rating"@en .

:description a owl:DatatypeProperty ;
    rdfs:range xsd:string ;
    rdfs:label "description"@en .

:wikidataURI a owl:DatatypeProperty ;
    rdfs:range xsd:anyURI ;
    rdfs:label "wikidata URI"@en ;
    rdfs:comment "Linked Open Data reference to Wikidata" .

:dbpediaURI a owl:DatatypeProperty ;
    rdfs:range xsd:anyURI ;
    rdfs:label "DBpedia URI"@en ;
    rdfs:comment "Linked Open Data reference to DBpedia" .

:timestamp a owl:DatatypeProperty ;
    rdfs:range xsd:dateTime ;
    rdfs:label "timestamp"@en .

